{
  "name": "jupyterlab_sage2",
  "version": "0.2.1",
  "description": "A JupyterLab extension to integrate JupyterLab and SAGE2 into one scientific workflow.",
  "author": "Andrew Burks",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/out/*.*",
    "style/img/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "SAGE2"
  ],
  "scripts": {
    "build": "npm run buildts & npm run buildless",
    "buildts": "tsc",
    "buildless": "lessc style/src/index.less style/out/index.css",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepublish": "npm run build"
  },
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.10.0",
    "@jupyterlab/apputils": "^0.10.0",
    "@jupyterlab/launcher": "^0.10.0",
    "@jupyterlab/notebook": "^0.10.0",
    "@jupyterlab/running": "^0.10.0",
    "@jupyterlab/services": "^0.49.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/virtualdom": "^1.1.2",
    "@phosphor/widgets": "^1.5.0"
  },
  "devDependencies": {
    "less": "^2.7.2",
    "rimraf": "^2.6.2",
    "typescript": "~2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewTBurks/jupyterlab_sage2.git"
  },
  "license": "MIT"
}
